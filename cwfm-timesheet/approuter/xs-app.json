{
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/service/do/(.*)$",
            "target": "/service/$1",
            "destination": "srv-api-dest",
            "authenticationType": "none",
            "csrfProtection": true
        },
        {
            "source": "^/(.*)",
            "target": "$1",
            "destination": "srv-api-dest",
            "authenticationType": "none",
            "csrfProtection": true
        }
    ]
}